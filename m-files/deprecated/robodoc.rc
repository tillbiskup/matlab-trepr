# robodoc.rc for MATLAB/Octave file documentation
# (c) 2005, Till Biskup <till.biskup@physik.fu-berlin.de>
items:
	NAME
	COPYRIGHT
	SYNOPSIS
	USAGE
	FUNCTION
	DESCRIPTION
	PURPOSE
	AUTHOR
	MAINTAINER
	CREATION DATE
	MODIFICATION DATE
	VERSION
	REVISION
	DERIVED FROM
	KEYWORDS
	ARGUMENTS
	PARAMETERS
	SWITCHES
	INPUT
	OUTPUT
	DEPENDS ON
	RESULT
	RETURN VALUE
	EXAMPLE
	NOTES
	WARNINGS
	ERRORS
	BUGS
	TODO
	TASKS
	TASK
	PORTABILITY
	SEE ALSO
	SOURCE
	INPUT PARAMETERS
	OUTPUT PARAMETERS
options:
    --src ./
    --doc ../doc/ROBODoc
    --html
    --multidoc
    --cmode
    --sections
    --nosource
ignore files:
    README
    CVS
	.svn
